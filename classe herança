public class Funcionario implements Atendimento {
    protected String nome;
    protected String cargo;

    public Funcionario(String nome, String cargo) {
        this.nome = nome;
        this.cargo = cargo;
    }

    @Override
    public void realizarAtendimento() {
        System.out.println(nome + " (Cargo: " + cargo + ") est√° atendendo um cliente.");
    }
}
